       print("\x1b[1;39mâ€”" * 60)
    
   
        
       secim = input(" \033[1;39m  [+] Seciminiz : ")

       if secim == "1":
            for i in range(100):
                Thread(target=get_username).start()  
       elif secim == "2":
            Sajad()        
       elif secim == "3":
